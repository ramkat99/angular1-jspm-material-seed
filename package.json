{
  "name": "angular1-jspm-material-seed",
  "license": "GPL-3.0",
  "version": "0.1.0",
  "description": "Seed Project for Angular1, JSPM, Material and GULP",
  "private": true,
  "scripts": {
    "start": "gulp serve",
    "build": "gulp build",
    "jshint": "gulp jshint",
    "htmlhint": "gulp htmlhint",
    "test": "gulp config --env=TEST --mock=false && gulp test:unit",
    "test-unit": "npm test",
    "test-e2e": "gulp test:e2e",
    "outdated": "npm outdated --depth 0",
    "deploy": "gulp gh-pages",
    "postinstall": "jspm install",
    "postmerge": "npm install",
    "prepush": "npm shrinkwrap --dev",
    "metrics:compile_source": "./node_modules/.bin/babel --optional runtime --stage=1 src/ --out-dir build/metrics/src",
    "metrics": "npm run metrics:compile_source && ./node_modules/.bin/plato -r -l .jshintrc -d build/metrics/report build/metrics/src"
  },
  "author": {
    "name": "Manie Coetzee",
    "url": "http://www.xibsolutions.com"
  },
  "jspm": {
    "directories": {
      "lib": "src"
    },
    "configFile": "jspm.conf.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.8",
      "angular-animate": "github:angular/bower-angular-animate@^1.5.8",
      "angular-material": "github:angular/bower-material@^1.1.1",
      "angular-material-icons": "npm:angular-material-icons@^0.7.1",
      "angular-messages": "github:angular/bower-angular-messages@^1.5.8",
      "angular-route": "github:angular/bower-angular-route@^1.5.8",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.5.8",
      "css": "github:systemjs/plugin-css@^0.1.32",
      "material-design-icons": "github:google/material-design-icons@^3.0.1",
      "text": "github:systemjs/plugin-text@^0.0.9"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.17.5",
    "connect-modrewrite": "^0.9.0",
    "conventional-changelog": "^1.1.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-bump": "^2.5.0",
    "gulp-bytediff": "^1.0.0",
    "gulp-cache": "^0.4.5",
    "gulp-cdnizer": "^1.1.7",
    "gulp-changed": "^1.3.2",
    "gulp-filter": "^4.0.0",
    "gulp-flatten": "^0.3.1",
    "gulp-header": "^1.8.8",
    "gulp-htmlhint": "^0.3.1",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.1.0",
    "gulp-inject": "^4.1.0",
    "gulp-istanbul-enforcer": "^1.0.3",
    "gulp-jshint": "^2.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-minify-html": "^1.0.6",
    "gulp-protractor": "^3.0.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^2.3.2",
    "gulp-size": "^2.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-usemin": "^0.3.25",
    "gulp-util": "^3.0.7",
    "jshint-stylish": "^2.2.1",
    "jspm": "^0.16.47",
    "karma": "^1.3.0",
    "moment": "^2.15.2",
    "plato": "^1.7.0",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "semver": "^5.3.0"
  }
}
